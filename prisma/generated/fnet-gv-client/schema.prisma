generator client {
  provider      = "prisma-client-js"
  output        = "./generated/fnet-gv-client"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
  provider     = "mysql"
  url          = env("FNET_GV_DATABASE_URL")
  relationMode = "prisma"
